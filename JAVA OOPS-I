interface ab
{
    public void disp();
}
abstract class at
{
    abstract void fun1();
    abstract void fun2();
}
class A implements ab
{
    A()
    {
        System.out.println("Base");
    }
    public void disp(){}
}
class B extends at
{
    B()
    {
        System.out.println("Derived");
    }
    public void fun1(){System.out.println("Hi1 Derived!");}
    public void fun2(){System.out.println("Hi2 Derived!");}
}
class Main
{
    public static void main(String a[])
    {
        new B();
    }
}
